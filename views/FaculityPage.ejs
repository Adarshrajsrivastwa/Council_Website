    <style>
        @font-face {
          font-family: Bristone;
          src: url(/Fonts/Bristone.ttf);
        }
        @font-face {
            font-family: Gilroy-black;
            src: url(/Fonts/Gilroy-Black.ttf);
        }
        @font-face {
             font-family:GilroyMedium ;
             src: url(/Fonts/Gilroy-Medium.ttf);
        }
       @font-face {
            font-family: Bebrush;
            src: url(/Fonts/Bebrush.otf);
        }
        @font-face {
            font-family: Affectsides;
            src: url(/Fonts/Affectsides.ttf);
        }
        :root {
            --primary-color: #4ecdc4;
            --accent-color: #ff3366;
            --dark-bg: #0a0a0a;
            --card-bg: rgba(255, 255, 255, 0.03);
            --text-primary: #ffffff;
            --text-secondary: rgba(255, 255, 255, 0.7);
            --gradient-primary: linear-gradient(45deg, #4ecdc4, #ff3366);
        }
        
        body {
            background-size: cover;
            position: relative;
            font-family: GilroyMedium;
            background-color:black;
            color: white;
            display: flex;
            flex-direction: column;
            align-items: center;
            margin: 0;
            padding: 0;
            width: 100%;
        }
        body::-webkit-scrollbar{
        display: none;
      }
        .container {
        width: 100%;
        max-width: 1400px;
        padding: 10px;
        background-size: cover;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        text-align: center;
        box-sizing: border-box;
        position: relative;
        animation: fadeIn 2s;
        margin: 0 auto;
        }

        @keyframes fadeIn {
            0% {
                opacity: 0;
            }
            100% {
                opacity: 1;
            }
        }

        .heading-barriers {
            font-family: Bebrush;
            margin: 5.5vw auto 2vw;
            letter-spacing: 1vw;
            font-weight: 400;
            width: fit-content;
            padding: 0 2rem;
            position: relative;
        }
        .heading-barriers::after {
        content: '';
        position: absolute;
        bottom: -10%;
        left: 0;
        width: 100%;
        height: 3px;
        background: linear-gradient(45deg, #4ecdc4, #ff3366) !important;
        border-radius: 2px;
      }
       

        .faculty-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            grid-gap: 20px;
            animation: fadeIn 2s;
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 20px;
            margin-top: 5vw;
        }

        .faculty-member {
            background-color: rgba(78, 205, 196, 0.05) !important;
            border: 1px solid rgba(78, 205, 196, 0.1) !important;
            padding: 15px;
            border-radius: 15px;
            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
            text-align: center;
            animation: fadeIn 2s;
            display: flex;
            flex-direction: column;
            align-items: center;
            width: 100%;
        }

        .faculty-member img {
            width: 120px;
            height: 120px;
            border-radius: 50%;
            object-fit: cover;
            margin-bottom: 15px;
            border: 3px solid var(--primary-color);
            box-shadow: 0 0 20px rgba(78, 205, 196, 0.2);
        }

        .faculty-member h3 {
            margin-bottom: 5px;
            font-size: 22px;
            font-weight: 200;
            color: var(--text-primary);
        }

        .faculty-member p {
            font-size: 0.8rem;
            color: var(--primary-color) !important;
            margin-bottom: 0.3rem;
            font-weight: 400;
        }

        .member-quote {
            font-size: 0.8rem;
            color: var(--text-secondary) !important;
            font-style: italic;
            line-height: 1.3;
            margin-top: 0.3rem;
            opacity: 0.9;
            max-width: 90%;
            text-align: center;
            background: rgba(78, 205, 196, 0.05);
            padding: 8px;
            border-radius: 8px;
            border: 1px solid rgba(78, 205, 196, 0.1);
        }

        @media (max-width: 1200px) {
            .faculty-grid {
                grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
                padding: 0 15px;
            }
        }

        .sub-faculity-container{
        width: 100%;
        max-width: 1400px;
        padding: 10px;
        /* background-color: #000000; */
        background-size: cover;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        text-align: center;
        box-sizing: border-box;
        position: relative;
        animation: fadeIn 2s;
        margin: 0 auto;
        }
        

        .sub-faculty-grid{
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            grid-gap: 15px;
            padding: 0 20px;
        }

        .sub-faculty-member {
            padding: 15px;
        }

        .sub-faculty-member .image {
            width: 90px;
            height: 90px;
        }

        .sub-faculty-member .h3 {
            /* color: black; */
            margin-bottom: 5px;
            font-size: 20px;
            font-weight: 100;
        }

        .sub-faculty-member .p {
            font-size: 16px;
            font-weight: 800;
            font-style: unset;
            font-family: 'Courier New', Courier, monospace;
            margin-top: 10px;
        }
        @media (max-width: 1200px) {
            .sub-faculty-grid {
                grid-template-columns: repeat(auto-fit, minmax(220px, 1fr));
            }
        }

        @media (max-width: 992px) {
            .faculty-member img {
                width: 110px;
                height: 110px;
            }
            
            .faculty-member h3 {
                font-size: 20px;
            }
            
            .member-quote {
                font-size: 0.75rem;
            }
        }

        @media (max-width: 768px) {
            .container, .sub-faculity-container {
                padding: 5px;
            }
            
            .faculty-grid {
                grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
                grid-gap: 15px;
                padding: 0 10px;
            }
            
            .sub-faculty-grid {
                grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
                padding: 0 10px;
            }
            
            .heading-barriers {
                margin-top: 12vw;
                padding: 0 1rem;
            }
        }

        @media (max-width: 576px) {
            .faculty-grid {
                grid-template-columns: 1fr;
                padding: 0 8px;
            }
            
            .sub-faculty-grid {
                grid-template-columns: repeat(auto-fit, minmax(160px, 1fr));
            }
            
            .faculty-member {
                padding: 12px;
            }
            
            .faculty-member img {
                width: 100px;
                height: 100px;
            }
            
            .heading-barriers {
                margin-top: 15vw;
                padding: 0 0.5rem;
            }
            
            .member-quote {
                font-size: 0.7rem;
                padding: 6px;
            }
        }

        @media (max-width: 320px) {
            .faculty-grid, .sub-faculty-grid {
                padding: 0 5px;
                grid-gap: 10px;
            }
            
            .faculty-member img {
                width: 90px;
                height: 90px;
            }
            
            .faculty-member h3 {
                font-size: 18px;
            }
        }

        /* Add support for landscape mode */
        @media (max-height: 500px) and (orientation: landscape) {
            .container, .sub-faculity-container {
                padding: 5px;
                margin-top: 70px;
            }
            
            .heading-barriers {
                margin-top: 8vw;
            }
            
            .faculty-grid {
                grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            }
        }

    </style>
</head>
