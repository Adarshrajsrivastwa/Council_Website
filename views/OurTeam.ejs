   <style>
    @font-face {
        font-family: Gilroy-Bold;
        src: url(/Fonts/Gilroy-Bold.ttf);
    }
    @font-face {
        font-family: Bristone;
        src: url(/Fonts/Bristone.ttf);
    }
    @font-face {
        font-family:Bebrush;
        src: url(/Fonts/Bebrush.otf);
    }
    
    body {
        background-color: white;
        color: #2a2a45;
        font-family: GilroyMedium, sans-serif;
        text-align: center;
    }
    body::-webkit-scrollbar {
    display: none;
    }
    .container {
        margin-top: 8vh;
        padding: 0 3vw;
    }
    .heading-bearer {
        margin: 2rem auto;
        position: relative;
        width: fit-content;
        padding-bottom: 1rem;
    }
    .heading-bearer h1 {
        font-family: Bebrush;
        font-size: 3.2vw;
        letter-spacing: 0.8vw;
        color: #2a2a45;
        margin-top:6vw;
        font-weight: 400;
    }
    .heading-bearer::after {
        content: '';
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        height: 2px;
        background: #2a2a45;
    }
    .team-grid {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        gap: 2rem;
        padding: 1rem;
        animation: fadeIn 1s ease;
    }
    .flip-card {
        background-color: transparent;
        width: 300px;
        height: 380px;
        perspective: 1000px;
        margin: 1rem;
    }
    .flip-card-inner {
        position: relative;
        width: 100%;
        height: 100%;
        text-align: center;
        transition: transform 0.6s;
        transform-style: preserve-3d;
    }
    .flip-card:hover .flip-card-inner {
        transform: rotateY(180deg);
    }
    .flip-card-front, .flip-card-back {
        position: absolute;
        width: 100%;
        height: 100%;
        -webkit-backface-visibility: hidden;
        backface-visibility: hidden;
        border-radius: 15px;
        box-shadow: 0 4px 15px rgba(42, 42, 69, 0.08);
    }
    .flip-card-front {
        background-color: #f8f9fa;
        padding: 1.5rem;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
    }
    .flip-card-front img {
        width: 180px;
        height: 180px;
        border-radius: 50%;
        object-fit: cover;
        border: 3px solid #2a2a45;
        box-shadow: 0 4px 15px rgba(42, 42, 69, 0.15);
        margin-bottom: 1rem;
    }
    .flip-card-front h3 {
        font-family: Gilroy-Bold;
        font-size: 1.4rem;
        color: #2a2a45;
        margin: 0.5rem 0;
    }
    .flip-card-back {
        background-color: #2a2a45;
        color: white;
        transform: rotateY(180deg);
        padding: 1.5rem;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
    }
    .flip-card-back p {
        font-family: GilroyMedium;
        font-size: 1.2rem;
        margin-bottom: 1.5rem;
    }
    .social-button {
        display: flex;
        gap: 0.8rem;
        margin-top: 1rem;
    }
    .icon-button {
        width: 45px;
        height: 45px;
        border-radius: 50%;
        background: white;
        display: flex;
        align-items: center;
        justify-content: center;
        cursor: pointer;
        position: relative;
        overflow: hidden;
    }
    .icon-button i {
        font-size: 1.2rem;
        z-index: 1;
        transition: color 0.3s ease;
    }
    .icon-button span {
        position: absolute;
        width: 0;
        height: 0;
        opacity: 0;
        border-radius: 50%;
        transition: all 0.3s ease;
    }
    .icon-button:hover span {
        width: 100%;
        height: 100%;
        opacity: 1;
    }
    .twitter span { background-color: #4099ff; }
    .facebook span { background-color: #3B5998; }
    .linkedin span { background-color: #0077b5; }
    .instagram span { background-color: #bc2a8d; }
    .github span { background-color: #333; }
    .icon-button:hover i {
        color: white;
    }
    @media (max-width: 1024px) {
        .heading-bearer h1 {
            font-size: 4vw;
        }
        
        .flip-card {
            width: 280px;
            height: 360px;
        }
    }
    @media (max-width: 768px) {
        .container {
            margin-top: 6vh;
        }
        
        .heading-bearer h1 {
            font-size: 5vw;
            letter-spacing: 0.6vw;
        }
        
        .flip-card {
            width: 260px;
            height: 340px;
            margin: 0.8rem;
        }
        
        .flip-card-front img {
            width: 160px;
            height: 160px;
        }
    }
    @media (max-width: 576px) {
        .container {
            margin-top: 5vh;
            padding: 0 2vw;
        }
        
        .heading-bearer h1 {
            font-size: 6vw;
            letter-spacing: 0.4vw;
            margin-top: 15vw;
        }
        
        .flip-card {
            width: 90vw;
            height: 380px;
            margin: 0.5rem;
        }
        
        .flip-card-front img {
            width: 200px;
            height: 200px;
        }
        
        .team-grid {
            gap: 1rem;
        }
    }
    /* Footer and Button Styles */
    footer {
        text-align: center;
        background-color: #23233e;
        color: #ddd;
        padding: 15px;
        width: 100%;
    }

    .button {
        background: linear-gradient(to right, #2a2a45, #23233e);
        color: white;
        padding: 10px 25px;
        border: none;
        border-radius: 25px;
        cursor: pointer;
        font-family: GilroyMedium;
        font-size: 16px;
        box-shadow: 0 4px 15px rgba(42, 42, 69, 0.2);
        transition: all 0.3s ease;
    }

    .button:hover {
        transform: translateY(-2px);
        box-shadow: 0 6px 20px rgba(42, 42, 69, 0.3);
        background: linear-gradient(to right, #23233e, #2a2a45);
    }

    a {
        text-decoration: none;
        color: black  /* This will keep the text color same as its parent */
    }

    /* If you want to specifically target social media links */
    .social-button a {
        text-decoration: none;
    }
</style>
