       /* Import Custom Fonts */
            @font-face {
                font-family: Bebrush;
                src: url(/Fonts/Bebrush.otf);
            }

            @font-face {
                font-family: Gilroy-Black;
                src: url(/Fonts/Gilroy-Black.ttf);
            }

            @font-face {
                font-family: Bristone;
                src: url(/Fonts/Bristone.ttf);
            }

            /* Global Styling */
            body {
                background: linear-gradient(256deg, #FFDEE9 0%, #B5FFFC 100%);
            }

            /* Event Container */
            .event-space {
                display: flex;
                flex-wrap: wrap;
                margin: 10px;
                gap: 22px;
                align-items: center;
                min-height: 100vh;
                justify-content: center;
            }

            /* Flip Card */
            .flip-card {
                background-color: transparent;
                width: 250px;
                height: 250px;
                perspective: 1000px; /* Adds depth effect */
                font-family: sans-serif;
                /* box-shadow:  1vw 1vw 50px black; */
            }

            .title {
                font-size: 1.2rem;
                font-weight: 500;
                text-align: center;
                margin: 0;
                font-family: Bristone;
                margin-bottom: 1vw;
            }

            .flip-card-inner {
                position: relative;
                width: 100%;
                height: 100%;
                text-align: center;
                transition: transform 0.8s;
                transform-style: preserve-3d;
            }

            .flip-card:hover .flip-card-inner {
                transform: rotateY(180deg);
            }

            /* Front & Back Styling */
            .flip-card-front,
            .flip-card-back {
                box-shadow: 0 8px 14px 0 rgba(0, 0, 0, 0.2);
                position: absolute;
                display: flex;
                flex-direction: column;
                justify-content: center;
                align-items: center;
                width: 100%;
                height: 100%;
                -webkit-backface-visibility: hidden;
                backface-visibility: hidden;
                /* border: 1px solid rgb(0, 0, 0); */
                border-radius: 1rem;
                overflow: hidden;
            }

            .flip-card-front {
                background: linear-gradient(256deg, #FFDEE9 0%, #B5FFFC 100%);
                color: black;
                z-index: 2; /* Ensures it's on top initially */
                padding: 1vw;
            }

            .flip-card-back {
                background: linear-gradient(256deg, #FFDEE9 0%, #B5FFFC 100%);
                color: white;
                transform: rotateY(180deg);
                z-index: 1;
            }

            .flip-card-back img {
                width: 100%;
                border: 1px solid black; /* Proper border syntax */
            }

            /* Heading Styling */
            .heading1 {
                text-align: center;
                margin-bottom: 15px;
                padding: 10px;
                font-size: 2rem;
                margin-top: 5.2vw;
                font-family: Bebrush;
                letter-spacing: 3px;
                color: rgb(20, 20, 20);
            }

            /* Responsive Font Size for Heading */
            @media (min-width: 200px) and (max-width: 999px) {
                .heading1 {
                    font-size: 0.8rem;
                }
            }


    </style>
</head>
